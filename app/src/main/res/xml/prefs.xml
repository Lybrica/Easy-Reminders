<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <CheckBoxPreference
        android:title="Vibrate"
        android:key="norm_vibration"
        android:summary="Enable normal notification vibration"
        android:defaultValue="true">
    </CheckBoxPreference>

    <CheckBoxPreference
        android:title="Alarm vibrate"
        android:key="alarm_vibration"
        android:summary="Enable insistent notification vibration">
    </CheckBoxPreference>

    <PreferenceScreen
        android:key="dialog_preference"
        android:title="Insistent notification volume">
    </PreferenceScreen>

</PreferenceScreen>